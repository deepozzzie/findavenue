    var url = form.attr('action');

    $.ajax({
      type: "POST",
      url: url,
      data: form.serialize(), // serializes the form's elements.
      success: function(data)
      {
        document.getElementById('submit_contact_us_form').style.display="none";
        document.getElementById('submit_contact_form').style.display="block";

      }
    });


  });
</script>


