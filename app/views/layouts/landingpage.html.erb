<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>

<script>
  $("#pre-register").submit(function(e) {

    e.preventDefault(); // avoid to execute the actual submit of the form.

    var form = $(this);
    var url = form.attr('action');

    $.ajax({
      type: "POST",
      url: url,
      data: form.serialize(), // serializes the form's elements.
      success: function(data)
      {
        document.getElementById('submit-pre-request_venue').style.display="none";
        document.getElementById('confirmation-pre-reg').style.display="block";

      }
    });


  });

  //Request Venue Details
  $("#request_company").submit(function(e) {
